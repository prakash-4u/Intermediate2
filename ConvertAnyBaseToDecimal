import java.util.*;
public class Main  {
 public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int a=sc.nextInt();
      int b=3,t=0,ans=0;
      while(a>0){
          int n=a%10;
          ans+=(n*(Math.pow(b,t)));
          t++;
          a=a/10;
      }
      System.out.println(ans);
    }
}